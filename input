import java.util.Scanner;

public class input
{
  public static void main(String[] args)
  {
    int[] num = new int[4];
    Scanner in = new Scanner(System.in);
    System.out.print("What's your favourate number betwwen 1 to 100? ");
    for (int i = 0; i < num.length; num++)
    {
      num[i] = in.nextInt();
    }
    for (int i : num)
    {
      System.out.println(i);
    }
  }
}
